#include <stdio.h>

int main() {
    int a, b;
    printf("Enter First Number: ");
    scanf("%d",&a);
    printf("Enter Second Number: ");
    scanf("%d",&b);

    printf("Addition = %d\n", a + b);
    printf("Subtraction = %d\n", a - b);
    printf("Multiplication = %d\n", a * b);

    if (b != 0)
        printf("Division = %d\n", a / b);
    else
        printf("Division not possible (denominator is 0)\n");

    return 0;
}
