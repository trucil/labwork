# include <stdio.h>

int main()
{
    int i;
    printf("Using for loop ; \n");
    for(i=1;i<11;i++)
    {
        printf("%d\n",i);
    }
    
    printf("Using while loop ; \n");
    i=1;
    while(i<=10)
    {
        printf("%d\n",i);
        i++;
    }
    
    i=1;
    printf("Using do while loop ; \n");
    do{
        printf("%d\n",i);
        i++;
    }while(i<=10);
}
