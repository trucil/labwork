# include <stdio.h>

int main()
{
    int i;
    printf("Using for loop ; \n");
    for(i=1;i<20;i+=2)
    {
        printf("%d\n",i);
    }
    
    printf("Using while loop ; \n");
    i=1;
    while(i<=20)
    {
        printf("%d\n",i);
        i+=2;
    }
    
    i=1;
    printf("Using do while loop ; \n");
    do{
        printf("%d\n",i);
        i+=2;
    }while(i<=20);
}
